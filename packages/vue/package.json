{
  "name": "@cain-group/cain-ui-vue",
  "sideEffects": false,
  "version": "0.0.4",
  "author": "cain-group",
  "license": "MIT",
  "description": "Vue specific wrapper for Cain UI",
  "repository": {
    "type": "git",
    "url": "https://github.com/cain-group/cain-ui.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "test": "echo 'vue: no tests yet'",
    "build2": "npm run clean && npm run build:cjs && npm run build:esm",
    "build": "npm run clean && npm run compile",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "build:esm": "tsc --module es6 --outDir dist/esm",
    "compile": "npm run tsc",
    "clean": "rimraf dist",
    "tsc": "tsc -p ."
  },
   "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@vue/test-utils": "^2.0.0-rc.6",
    "jest": "^26.1.0",
    "rimraf": "^2.6.3",
    "rollup": "^1.21.2",
    "rollup-plugin-node-resolve": "^5.2.0",
    "typescript": "^4.3.2",
    "vue": "^3.1.1"
  },
  "dependencies": {
    "@cain-group/cain-ui": "0.0.4"
  },
  "peerDependencies": {
    "vue": "^2.6.12"
  },
  "jest": {
    "preset": "ts-jest",
    "setupFilesAfterEnv": [
      "<rootDir>/setupTests.ts"
    ],
    "testPathIgnorePatterns": [
      "node_modules",
      "dist"
    ],
    "globals": {
      "ts-jest": {
        "diagnostics": {
          "warnOnly": true
        }
      }
    }
  }
}
