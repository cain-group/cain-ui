    "@angular/compiler-cli": "^13.3.0",
    "@angular/core": "^13.3.0",
    "ng-packagr": "^13.3.0",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.5",
    "typescript": "4.4.4"
